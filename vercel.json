{
    "version": 2,
    "buildCommand": "ng build",
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/api"
        }
    ],
    "functions": {
        "api/index.js": {
            "runtime": "nodejs20.x",
            "includeFiles": "dist/angular_app/**"
        }
    }
}